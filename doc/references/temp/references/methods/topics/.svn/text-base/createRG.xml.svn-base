<?xml version="1.0" encoding="utf-8"?><?workdir /C:\SequoiaDB\doc\references\temp\references\methods\topics?><?path2project ..\..\?><topic xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" id="references_method_createRG" ditaarch:DITAArchVersion="1.2" domains="(topic hi-d)                             (topic ut-d)                             (topic indexing-d)                            (topic hazard-d)                            (topic abbrev-d)                            (topic pr-d)                             (topic sw-d)                            (topic ui-d)    " class="- topic/topic " xtrf="C:\SequoiaDB\doc\references\methods\topics\createRG.dita" xtrc="topic:1">
  <title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\methods\topics\createRG.dita" xtrc="title:1">db.createRG()</title>
  <body class="- topic/body " xtrf="C:\SequoiaDB\doc\references\methods\topics\createRG.dita" xtrc="body:1">
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\references\methods\topics\createRG.dita" xtrc="section:1"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\methods\topics\createRG.dita" xtrc="title:2">db.createRG(&lt;groupname&gt;)</title>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\methods\topics\createRG.dita" xtrc="p:1">新建一个分片组。创建后系统自动为分片组分配一个GroupId</p>
    </section>
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\references\methods\topics\createRG.dita" xtrc="section:2"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\methods\topics\createRG.dita" xtrc="title:3">参数描述</title>
