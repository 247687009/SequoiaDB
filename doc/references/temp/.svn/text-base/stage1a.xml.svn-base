            <xref class="- topic/xref " href="#unique_31" ohref="methods\topics\createIndex_en.xml" type="topic" xtrc="xref:7" xtrf="C:\SequoiaDB\doc\references\sqlmapping\topics\sqlmapping_en.dita"><?ditaot usertext?>createIndex() ,</xref>
            <xref class="- topic/xref " href="#unique_68" ohref="..\administration\concepts\database\topics\index_en.xml" type="topic" xtrc="xref:8" xtrf="C:\SequoiaDB\doc\references\sqlmapping\topics\sqlmapping_en.dita"><?ditaot usertext?>index</xref>
            </stentry>
        </strow>
        <strow class="- topic/strow " xtrc="strow:12" xtrf="C:\SequoiaDB\doc\references\sqlmapping\topics\sqlmapping_en.dita">
