<?xml version="1.0" encoding="utf-8"?><?workdir /C:\SequoiaDB\doc\references\temp\references\sql_grammar\topics?><?path2project ..\..\?><topic xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" id="references_sql_dropindex" ditaarch:DITAArchVersion="1.2" domains="(topic hi-d)                             (topic ut-d)                             (topic indexing-d)                            (topic hazard-d)                            (topic abbrev-d)                            (topic pr-d)                             (topic sw-d)                            (topic ui-d)    " class="- topic/topic " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="topic:1">
  <title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="title:1">sql drop index</title>
  <body class="- topic/body " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="body:1">
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="section:1"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="title:2">drop index 语句</title>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="p:1">用于删除集合中的索引。</p>
    </section>
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="section:2"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="title:3">语法</title>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="p:2"><codeblock xml:space="preserve" class="+ topic/pre pr-d/codeblock " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="codeblock:1">drop index &lt;index_name&gt; on &lt;cs_name&gt;.&lt;cl_name&gt;</codeblock></p>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="p:3">&lt;index_name&gt;：索引名</p>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="p:4">&lt;cs_name&gt;：集合空间名</p>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="p:5">&lt;cl_name&gt;：集合名</p>
    </section>
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="section:3"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="title:4">示例</title>
      <ul class="- topic/ul " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="ul:1">
        <li class="- topic/li " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="li:1"><p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="p:6">删除集合空间foo中集合bar下名为test_index的索引名</p>
          <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="p:7"><codeblock xml:space="preserve" class="+ topic/pre pr-d/codeblock " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_index.dita" xtrc="codeblock:2">db.execUpdate("drop index test_index on foo.bar") //等价于db.foo.bar.dropIndex("test_index")</codeblock></p>
        </li>
      </ul>
    </section>
  </body>
