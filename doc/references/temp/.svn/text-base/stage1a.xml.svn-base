            </stentry>
        </strow>
        <strow class="- topic/strow " xtrc="strow:11" xtrf="C:\SequoiaDB\doc\references\sqlmapping\topics\sqlmapping_en.dita">
          <stentry class="- topic/stentry " xtrc="stentry:29" xtrf="C:\SequoiaDB\doc\references\sqlmapping\topics\sqlmapping_en.dita">create index index_stu_id<p class="- topic/p " xtrc="p:12" xtrf="C:\SequoiaDB\doc\references\sqlmapping\topics\sqlmapping_en.dita">on student (stu_id)</p></stentry>
