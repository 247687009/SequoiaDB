<?xml version="1.0" encoding="utf-8"?><?workdir /C:\SequoiaDB\doc\references\temp\references\sql_grammar\topics?><?path2project ..\..\?><topic xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" id="references_sql_dropCS" ditaarch:DITAArchVersion="1.2" domains="(topic hi-d)                             (topic ut-d)                             (topic indexing-d)                            (topic hazard-d)                            (topic abbrev-d)                            (topic pr-d)                             (topic sw-d)                            (topic ui-d)    " class="- topic/topic " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="topic:1">
  <title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="title:1">sql drop collectionspace</title>
  <body class="- topic/body " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="body:1">
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="section:1"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="title:2">drop collectionspace 语句</title>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="p:1">用于删除数据库中的集合空间。</p>
    </section>
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="section:2"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="title:3">语法</title>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="p:2"><codeblock xml:space="preserve" class="+ topic/pre pr-d/codeblock " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="codeblock:1">drop collectionspace &lt;cs_name&gt;</codeblock></p>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="p:3">&lt;cs_name&gt;参数为集合空间名，集合空间名必须在数据库中存在。</p>
    </section>
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="section:3"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="title:4">示例</title>
      <ul class="- topic/ul " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="ul:1">
        <li class="- topic/li " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="li:1"><p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="p:4">删除名为foo的集合空间</p>
          <p class="- topic/p " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="p:5"><codeblock xml:space="preserve" class="+ topic/pre pr-d/codeblock " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cs.dita" xtrc="codeblock:2">db.execUpdate("drop collectionspace foo") //等价于db.dropCS("foo")</codeblock></p>
        </li>
      </ul>
    </section>
  </body>
