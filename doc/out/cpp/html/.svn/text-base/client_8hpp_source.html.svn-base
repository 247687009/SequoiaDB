<div class="line"><a name="l00722"></a><span class="lineno">  722</span>&#160;                     <span class="keyword">const</span> bson::BSONObj &amp;selected  = _sdbStaticObject,</div>
<div class="line"><a name="l00723"></a><span class="lineno">  723</span>&#160;                     <span class="keyword">const</span> bson::BSONObj &amp;orderBy   = _sdbStaticObject,</div>
