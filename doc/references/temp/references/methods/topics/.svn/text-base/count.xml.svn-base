        <sthead class="- topic/sthead " xtrf="C:\SequoiaDB\doc\references\methods\topics\count.dita" xtrc="sthead:1">
          <stentry class="- topic/stentry " xtrf="C:\SequoiaDB\doc\references\methods\topics\count.dita" xtrc="stentry:1">参数名</stentry>
          <stentry class="- topic/stentry " xtrf="C:\SequoiaDB\doc\references\methods\topics\count.dita" xtrc="stentry:2">参数类型</stentry>
          <stentry class="- topic/stentry " xtrf="C:\SequoiaDB\doc\references\methods\topics\count.dita" xtrc="stentry:3">描述</stentry>
          <stentry class="- topic/stentry " xtrf="C:\SequoiaDB\doc\references\methods\topics\count.dita" xtrc="stentry:4">是否必填</stentry>
