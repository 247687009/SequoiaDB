<?xml version="1.0" encoding="utf-8"?><?workdir /C:\SequoiaDB\doc\references\temp\administration\database\topics\snapshot?><?path2project ..\..\..\..\out_en\?><topic xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" id="administration_database_monitoring_snapshot_collections" ditaarch:DITAArchVersion="1.2" domains="(topic hi-d)                             (topic ut-d)                             (topic indexing-d)                            (topic hazard-d)                            (topic abbrev-d)                            (topic pr-d)                             (topic sw-d)                            (topic ui-d)    " class="- topic/topic " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="topic:1">
  <title class="- topic/title " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="title:1">Collection Snapshot</title>
  <body class="- topic/body " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="body:1">
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="section:1"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="title:2">Description</title>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="p:1">Collections snapshot contains all the non-temporary collections in current data nodes (It
        contains user collection in coord node). Each collection is recorded as a record.</p>
    </section>
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="section:2"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="title:3">Sign</title>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="p:2">SDB_SNAP_COLLECTIONS</p>
    </section>
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="section:3"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="title:4">Proper Node</title>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="p:3">All nodes</p>
    </section>
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="section:4"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="title:5">Field Information</title>
      <p class="- topic/p " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="p:4">Since information stored in data nodes and catalog node is not the same, collections list
        will return different structures according to the kind of the node:</p>
    </section>
    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="section:5"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\administration\database\topics\snapshot\collections_en.dita" xtrc="title:6">Coord Node Field Information</title>
