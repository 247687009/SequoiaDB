    <section class="- topic/section " xtrc="section:2" xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\select_en.dita"><title class="- topic/title " xtrc="title:3" xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\select_en.dita">Grammar</title>
      <p class="- topic/p " xtrc="p:2" xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\select_en.dita"><codeblock class="+ topic/pre pr-d/codeblock " xml:space="preserve" xtrc="codeblock:1" xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\select_en.dita">select * from &lt;cs_name&gt;.&lt;cl_name&gt;
