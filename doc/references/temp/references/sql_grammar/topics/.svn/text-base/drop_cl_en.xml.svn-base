    <section class="- topic/section " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cl_en.dita" xtrc="section:2"><title class="- topic/title " xtrf="C:\SequoiaDB\doc\references\sql_grammar\topics\drop_cl_en.dita" xtrc="title:3">Grammar</title>
