<div class="line"><a name="l00725"></a><span class="lineno">  725</span>&#160;                     INT64 numToSkip          = 0,</div>
<div class="line"><a name="l00726"></a><span class="lineno">  726</span>&#160;                     INT64 numToReturn        = -1,</div>
<div class="line"><a name="l00727"></a><span class="lineno">  727</span>&#160;                     INT32 flag               = 0</div>
<div class="line"><a name="l00728"></a><span class="lineno">  728</span>&#160;                   )</div>
<div class="line"><a name="l00729"></a><span class="lineno">  729</span>&#160;      {</div>
