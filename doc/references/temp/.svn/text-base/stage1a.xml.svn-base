        <strow class="- topic/strow " xtrc="strow:1" xtrf="C:\SequoiaDB\doc\references\methods\topics\createIndex_en.dita">
          <stentry class="- topic/stentry " xtrc="stentry:5" xtrf="C:\SequoiaDB\doc\references\methods\topics\createIndex_en.dita">name</stentry>
          <stentry class="- topic/stentry " xtrc="stentry:6" xtrf="C:\SequoiaDB\doc\references\methods\topics\createIndex_en.dita">string</stentry>
          <stentry class="- topic/stentry " xtrc="stentry:7" xtrf="C:\SequoiaDB\doc\references\methods\topics\createIndex_en.dita">Index name. It should be unique in a collection.</stentry>
