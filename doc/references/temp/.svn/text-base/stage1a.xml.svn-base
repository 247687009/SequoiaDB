    <section class="- topic/section " xtrc="section:2" xtrf="C:\SequoiaDB\doc\references\methods\topics\createIndex_en.dita"><title class="- topic/title " xtrc="title:3" xtrf="C:\SequoiaDB\doc\references\methods\topics\createIndex_en.dita">Parameter Description</title>
      <simpletable class="- topic/simpletable " frame="all" id="unique_31_Connect_42_references_method_createIndex_table" relcolwidth="1.0* 1.13* 5.67* 1.16*" xtrc="simpletable:1" xtrf="C:\SequoiaDB\doc\references\methods\topics\createIndex_en.dita">
